<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.aishwarya.myapplication" >


    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/appicon"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >

        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="@string/google_maps_key" />
        <meta-data
            android:name="com.facebook.sdk.ApplicationId"
            android:value="@string/facebook_app_id" />

        <activity
            android:name="com.facebook.FacebookActivity"
            android:configChanges="keyboard|keyboardHidden|screenLayout|screenSize|orientation"
            android:label="@string/app_name"
            android:theme="@android:style/Theme.Translucent.NoTitleBar" />
        <activity
            android:name=".MainActivity"
            android:label="List of All Countries" >
        </activity>
        <activity android:name=".CountryDetail" >
        </activity>
        <activity android:name=".MapActivity" >
        </activity>
        <activity android:name=".SplashActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".LoginActivity" >
        </activity>
        <activity android:name=".navigation.QuizAcitvity"
            android:theme="@style/AppTheme1">
        </activity>
        <activity android:name=".quizzes.CapitalQuizActivity"
            android:theme="@style/AppTheme1">
        </activity>
        <activity android:name=".quizzes.FlagQuizActivity"
            android:theme="@style/AppTheme1">
        </activity>
        <activity android:name=".floatingactionbutton.FloatingMainActivity"
            android:theme="@style/AppTheme">
        </activity>
    </application>

</manifest>
